{
  "name": "@haoqimao/tibox-cli",
  "version": "0.1.4",
  "description": "tibox",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "author": "张博 <zbjumper@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build:comment": "构建",
    "build": "npm run eslint && npm run prettier && rm -rf lib && tsc --build",
    "dev:comment": "启动开发环境",
    "dev": "ts-node-dev --inspect-brk --respawn --transpile-only src/index.ts",
    "eslint:comment": "使用 ESLint 检查并自动修复 src 目录下所有扩展名为 .ts 的文件",
    "eslint": "eslint --fix src --ext .ts --max-warnings=0",
    "prettier:comment": "自动格式化 src 目录下的所有 .ts 文件",
    "prettier": "prettier --write \"src/**/*.ts\"",
    "commit:comment": "引导设置规范化的提交信息",
    "commit": "cz",
    "pub": "yarn publish --registry=https://registry.npmjs.com"
  },
  "bin": {
    "tibox": "./bin/tibox.js"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {
    "cac": "^6.7.3",
    "chalk": "^4.1.1",
    "chokidar": "^3.5.2",
    "clear-console": "^1.1.0",
    "debug": "^4.3.2",
    "dotenv": "^10.0.0",
    "dotenv-expand": "^5.1.0",
    "esbuild": "^0.12.16",
    "fs-extra": "^10.0.0",
    "gulp": "^4.0.2",
    "gulp-changed": "^4.0.3",
    "gulp-debug": "^4.0.0",
    "gulp-filter": "^7.0.0",
    "gulp-if": "^3.0.0",
    "gulp-json-editor": "^2.5.6",
    "gulp-prune": "^2.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.3",
    "inquirer": "^8.1.0",
    "load-json-file": "^6.2.0",
    "lodash": "^4.17.21",
    "map-stream": "^0.0.7",
    "readline": "^1.3.0",
    "shelljs": "^0.8.4",
    "through2": "^4.0.2",
    "vinyl-fs": "^3.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@types/debug": "^4.1.7",
    "@types/fs-extra": "^9.0.12",
    "@types/gulp": "^4.0.9",
    "@types/gulp-changed": "^0.0.34",
    "@types/gulp-debug": "^2.0.32",
    "@types/gulp-filter": "^3.0.34",
    "@types/gulp-if": "^0.0.34",
    "@types/gulp-rename": "^2.0.1",
    "@types/inquirer": "^7.3.1",
    "@types/lodash": "^4.14.170",
    "@types/node": "*",
    "@types/shelljs": "^0.8.8",
    "@types/through2": "^2.0.36",
    "@types/vinyl-fs": "^2.4.11",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.27.0",
    "prettier": "^2.3.0",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.3.2",
    "undertaker": "^1.3.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
